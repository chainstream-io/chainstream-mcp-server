    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/typeorm": ">=9.0.0",
    "express": ">=4.0.0",
    "reflect-metadata": ">=0.1.14",
    "typeorm": ">=0.3.25",
    "zod": "^3.0.0",
    "zod-to-json-schema": ">=3.23.0"
  },
