    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "dependencies": {
    "@chainstream-io/dex": "^0.0.94",
